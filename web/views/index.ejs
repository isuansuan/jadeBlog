<% include header.ejs %>
<div class="span7" style="float: left;">

    <div id="myCarousel" class="carousel slide">
        <!-- 轮播（Carousel）指标 -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
            <li data-target="#myCarousel" data-slide-to="4"></li>
            <li data-target="#myCarousel" data-slide-to="5"></li>
        </ol>
        <!-- 轮播（Carousel）项目 -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="/images/slides/slide1.png" width=1250px height=800px alt="First slide">
            </div>
            <div class="item">
                <img src="/images/slides/slide2.png" width=1250px height=800px alt="Second slide">
            </div>
            <div class="item">
                <img src="/images/slides/slide3.png" width=1250px height=800px alt="Third slide">
            </div>
            <div class="item">
                <img src="/images/slides/slide4.png" width=1250px height=800px alt="fourth slide">
            </div>
            <div class="item">
                <img src="/images/slides/slide5.png" width=1250px height=800px alt="fifth slide">
            </div>
            <div class="item">
                <img src="/images/slides/slide6.png" width=1250px height=800px alt="sixth slide">
            </div>
        </div>
        <!-- 轮播（Carousel）导航 -->
        <a class="carousel-control left" href="#myCarousel"
           data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#myCarousel"
           data-slide="next">&rsaquo;</a>
    </div>

    <!--<div class="hero-unit">-->
    <!--<h1><%= mainpage.welcome.head[lang] %></h1>-->
    <!--<p><%= mainpage.welcome[lang] %></p>-->
    <!--<p><a href="http://my.csdn.net/dai_jing" class="btn btn-primary btn-large">Learn more &raquo;</a></p>-->
    <!--</div>-->
    <div class="row-fluid">
        <% for(var i = 0, len = mainpage.childPage.length;i < len;++i){ %>
        <div class="span4">
            <h2><%= mainpage.childPage[i][lang] %></h2>

            <p></p>

            <p><a class="btn" href="<%= mainpage.childPage[i].link %>" target="_blank">View details &raquo;</a></p>
        </div>
        <!--/span-->
        <% } %>
    </div>
    <!--/row-->
</div><!--/span-->

<div class="span3" style="border: 0 solid black;float: right;text-align: center;">
    <div style="text-align: center;font-family:'Microsoft YaHei';color:orangered;background-color: #272822;">
        <h2>广告招商</h2>
    </div>
    <div style="margin: 5px;">
        <img src="/images/adverts/ad1.png" >
    </div>
    <div style="margin: 5px;">
        <img src="/images/adverts/ad2.png" >
    </div>

</div>


